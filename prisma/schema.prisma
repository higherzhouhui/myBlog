generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id     Int     @id @default(autoincrement())
  name   String?
  token  String?
  header String?
  enail  String?
}

model Blog {
  id Int @id @default(autoincrement())
}
